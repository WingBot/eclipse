#ifndef INCLUDE_HANS_BRINGUP_AGVODOM_H_
#define INCLUDE_HANS_BRINGUP_AGVODOM_H_
#include "hans_bringup/data_definition.h"


typedef struct Coords    //平面坐标结构体
{
